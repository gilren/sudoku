{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "sudoku",
  "compatibility_date": "2026-02-12",
  "pages_build_output_dir": "./dist",
  "observability": {
    "enabled": true,
  },
  "compatibility_flags": ["nodejs_compat"],

  "routes": [
    {
      "pattern": "sudoku.renaudgillet.be",
      "custom_domain": true,
    },
  ],
}
